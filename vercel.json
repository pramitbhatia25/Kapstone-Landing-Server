{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "dest": "/server.js",
        "headers": [
          {
            "key": "Referrer-Policy",
            "value": "strict-origin"
          },
          {
            "key": "Access-Control-Allow-Origin",
            "value": "https://kapstone-landing.vercel.app"
          }
        ]
      }
    ]
  }
  